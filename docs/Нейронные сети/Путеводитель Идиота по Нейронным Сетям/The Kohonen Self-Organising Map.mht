<SCRIPT language=3DJavaScript>
var n =3D 2, NUMCELLS =3D 10;
var i,j;

// Set up array of weights, 2 inputs by 10 outputs
var w =3D new Array();
w[0] =3D new Array();
w[1] =3D new Array();
for (i =3D 0; i < n; i++)
 for (j =3D 0; j < NUMCELLS; j++)
  w[i][j] =3D Math.random() * 0.01;

// Array x holds the inputs for training and testing
var x =3D new Array ();

// Array d holds the differences during training
var d =3D new Array ();

var j_star;             // Identity of winning node
var LEARN_RATE =3D 0.01;  // Rate at which weights change

// Here is the code for the one-dimensional example

// Train the SOM
function train1 ()
{ var r_copy =3D parseInt(range_size.value); // Field value will be =
restored afterwards
  var N;
  var iter =3D parseInt(iterations.value); // Number of iterations for =
each range
  for (N =3D r_copy; N >=3D 0; N--)          // Decreases radius to 0 =
(single cell)
   { range_size.value =3D N;               // Display value in field
     for (count =3D 0; count < iter; count++)
      { iterations.value =3D count;        // Display value in field
        random_input(2);
        calculate_distances();
        select_minimum_distance();
        update_weights(N);
      }
   }
  range_size.value =3D r_copy;      // Restore the values in the fields
  iterations.value =3D iter;
  alert("Network trained");
}

// Pick a random pattern, High-Low or Low-High (equally likely)
function random_input (limit)
{ var which =3D Math.floor(Math.random() * limit);
  x[0] =3D Math.random() * 0.2;                // Set 2 low values
  x[1] =3D Math.random() * 0.2;
  if (which =3D=3D 0 || which =3D=3D 3)
   x[1] +=3D 0.8;
  if (which =3D=3D 1 || which =3D=3D 3)
   x[0] +=3D 0.8;
}

function calculate_distances ()
{ var i,j;
  for (j =3D 0; j < NUMCELLS; j++)
   { d[j] =3D 0.0;
     for (i =3D 0; i < n; i++)
      d[j] +=3D (x[i] - w[i][j]) * (x[i] - w[i][j]);
   }
}

function select_minimum_distance ()
{ var temp_d =3D 100000;      // dummy value
  var j;
  for (j =3D 0; j < NUMCELLS; j++)
   if (d[j] < temp_d)
    { temp_d =3D d[j];
      j_star =3D j;     // Tracks index of minimum distance
    }
}

function update_weights (N)
{ // Go through all the cells and only update those within N units of =
j_star
  var j;
  for (j =3D 0; j < NUMCELLS; j++)
   if (j >=3D j_star - N && j <=3D j_star + N)
    for (i =3D 0; i < n; i++)                // Update weights from all =
inputs
     w[i][j] +=3D LEARN_RATE * (x[i] - w[i][j]);
}

// Test the SOM on the current input
function test1 ()
{ x[0] =3D parseFloat(inp1.value);
  x[1] =3D parseFloat(inp2.value);
  // Calculate output for each cell
  for (j =3D 0; j < NUMCELLS; j++)
   { var sum =3D 0;     // Build up weighted sum of inputs
     for (i =3D 0; i < n; i++)
      sum +=3D x[i] * w[i][j];
     // Set the image based on the (rounded) output
     var rounded =3D Math.round(sum * 10 - 0.5);    // Always round down
     if (rounded > 9) // Ensures all values can be displayed
      rounded =3D 9;    // Normally this artificial ceiling is =
unnecessary
     document.images["pic" + j].src =3D "v" + rounded + ".gif";
   }
}

// Here is the code for the two-dimensional example
// Same as before, but functions have suffix '2'

var j_star_row, j_star_col;     // 2-d equivalent of j_star

// w2[i][j][k] =3D weight from input i to cell j,k
var w2 =3D new Array();
var i,j,k;
for (i =3D 0; i < n; i++)
 { w2[i] =3D new Array();
   for (j =3D 0; j < NUMCELLS; j++)
    w2[i][j] =3D new Array();
 }

// d2[j][k] =3D distance to cell j,k
var d2 =3D new Array();
for (j =3D 0; j < NUMCELLS; j++)
 d2[j] =3D new Array();

// Now set up the weights to small random values (0 to 0.01)
for (i =3D 0; i < n; i++)
 for (j =3D 0; j < NUMCELLS; j++)
  for (k =3D 0; k < NUMCELLS; k++)
   w2[i][j][k] =3D Math.random() * 0.01;

function train2 ()
{ var r_copy =3D parseInt(range_size.value);
  var N;
  var iter =3D parseInt(iterations2.value); // Number of iterations for =
each range
  for (N =3D r_copy; N >=3D 0; N--)          // Decreases radius to 0 =
(single cell)
   { for (count =3D 0; count < iter; count++)
      { random_input(4);
        calculate_distances2();
        select_minimum_distance2();
        update_weights2(N);
      }
   }
  alert("Network trained");
}

function calculate_distances2 ()
{ var i,j,k;
  for (j =3D 0; j < NUMCELLS; j++)
   for (k =3D 0; k < NUMCELLS; k++)
    { d2[j][k] =3D 0.0;
      for (i =3D 0; i < n; i++)
       d2[j][k] +=3D (x[i] - w2[i][j][k]) * (x[i] - w2[i][j][k]);
    }
}

function select_minimum_distance2 ()
{ var temp_d =3D 100000;      // dummy value
  var j,k;
  for (j =3D 0; j < NUMCELLS; j++)
   for (k =3D 0; k < NUMCELLS; k++)
    if (d2[j][k] < temp_d)
     { temp_d =3D d2[j][k];
       j_star_row =3D j;     // Tracks index of minimum distance
       j_star_col =3D k;
     }
}

function update_weights2 (N)
{ // Go through all the cells and only update those within N units of =
j_star_row, j_star_col
  var i,j,k;
  for (j =3D 0; j < NUMCELLS; j++)
   for (k =3D 0; k < NUMCELLS; k++)
    if (j >=3D j_star_row - N && j <=3D j_star_row + N &&
        k >=3D j_star_col - N && k <=3D j_star_col + N)
     for (i =3D 0; i < n; i++)                // Update weights from all =
inputs
      w2[i][j][k] +=3D LEARN_RATE * (x[i] - w2[i][j][k]);
}

// Test the SOM on the current input
function test2 ()
{ x[0] =3D parseFloat(inp2_1.value);
  x[1] =3D parseFloat(inp2_2.value);
  // Calculate output for each cell
  for (j =3D 0; j < NUMCELLS; j++)
   for (k =3D 0; k < NUMCELLS; k++)
    { var sum =3D 0;     // Build up weighted sum of inputs
      for (i =3D 0; i < n; i++)
       sum +=3D x[i] * w2[i][j][k];
      // Set the image based on the (rounded) output
      var rounded =3D Math.round(sum * 10 - 0.5);    // Always round =
down
      if (rounded > 9) // Ensures all values can be displayed
       rounded =3D 9;    // Normally this artificial ceiling is =
unnecessary
      document.images["pic" + j + "_" + k].src =3D "v" + rounded + =
".gif";
    }
}
